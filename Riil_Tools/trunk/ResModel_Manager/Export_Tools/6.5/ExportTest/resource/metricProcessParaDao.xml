<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.MetricProcessParaDao">

	<resultMap id="metricBindingResultMap" type="com.riil.base.binding.pojo.MetricProcessPara">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="metricBindingId" column="C_METRICBINDING_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="parameter" column="C_PARAMETER" javaType="string" jdbcType="VARCHAR" />
		<result property="sortId" column="C_SORT_ID" javaType="string" jdbcType="INTEGER" />
	</resultMap>
	<select id="select_by_all"  resultMap="metricBindingResultMap">
		SELECT C_ID,   C_METRICBINDING_ID, C_PARAMETER, C_SORT_ID
		FROM t_moni_metric_process_para
		ORDER BY C_SORT_ID ASC;
	</select>
	
	<select id="select_by_bingId" parameterType="String" resultMap="metricBindingResultMap">
		SELECT C_ID,   C_METRICBINDING_ID, C_PARAMETER, C_SORT_ID
		FROM t_moni_metric_process_para
		WHERE C_METRICBINDING_ID = #{metricBindingId ,jdbcType=VARCHAR}
		ORDER BY C_SORT_ID ASC;
	</select>
	
</mapper>
