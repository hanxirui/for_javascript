<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.CollectCmdDao">

	<resultMap id="collectCmdResultMap" type="com.riil.base.binding.pojo.CollectCmdPojo">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="collectCmdsId" column="C_CMDS_GROUP_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="index" column="C_INDEX" javaType="string" jdbcType="VARCHAR" />
		<result property="protocol" column="C_PROTOCOL" javaType="string" jdbcType="VARCHAR" />
		<result property="cmd" column="C_CMD" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	<select id="select_by_all"  resultMap="collectCmdResultMap">
		SELECT C_ID, C_CMDS_GROUP_ID,C_INDEX,C_PROTOCOL,C_CMD
		FROM
		t_moni_cmd
    </select>

	<select id="select_by_collectCmdsId" parameterType="string" resultMap="collectCmdResultMap">
		SELECT C_ID, C_CMDS_GROUP_ID,C_INDEX,C_PROTOCOL,C_CMD
		FROM
		t_moni_cmd
		WHERE C_CMDS_GROUP_ID = #{collectCmdsId ,jdbcType=VARCHAR}
    </select>
	
</mapper>
