<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.supportDao">

	<resultMap id="cmdSupportResultMap" type="com.riil.base.binding.pojo.CmdSupportPojo">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="cmdsGroupId" column="C_CMDS_GROUP_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="version" column="C_VERSION" javaType="string" jdbcType="VARCHAR" />
		<result property="rel" column="C_REL" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	<select id="select_by_all"  resultMap="cmdSupportResultMap">
		SELECT C_ID, C_CMDS_GROUP_ID,C_VERSION,C_REL
		FROM
		t_moni_cmds_support
    </select>
	<select id="select_by_cmdsId" parameterType="string" resultMap="cmdSupportResultMap">
		SELECT C_ID, C_CMDS_GROUP_ID,C_VERSION,C_REL
		FROM
		t_moni_cmds_support
		WHERE C_CMDS_GROUP_ID = #{cmdsGroupId ,jdbcType=VARCHAR}
    </select>
	
</mapper>
