<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.CmdFilterDao">

	<resultMap id="cmdFilterResultMap" type="com.riil.base.binding.pojo.CmdFilter">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="cmdId" column="C_CMD_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="dependecy" column="C_DEPENDECY" javaType="string" jdbcType="VARCHAR" />
		<result property="row" column="C_ROW" javaType="string" jdbcType="VARCHAR" />
		<result property="column" column="C_COLUMN" javaType="string" jdbcType="VARCHAR" />
		<result property="cmdParameterName" column="C_CMDPARAMETERNAME" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	<select id="select_by_all"  resultMap="cmdFilterResultMap">
		SELECT C_ID, C_CMD_ID,C_DEPENDECY,C_ROW,C_COLUMN,C_CMDPARAMETERNAME
		FROM
		t_moni_cmd_filters
    </select>
	
	<select id="select_by_collectCmdId" parameterType="string" resultMap="cmdFilterResultMap">
		SELECT C_ID, C_CMD_ID,C_DEPENDECY,C_ROW,C_COLUMN,C_CMDPARAMETERNAME
		FROM
		t_moni_cmd_filters
		WHERE C_CMD_ID = #{cmdId ,jdbcType=VARCHAR}
    </select>
	
</mapper>
