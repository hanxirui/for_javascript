<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.collFrequencyDao">

	<resultMap id="dictCollFrequencyResultMap" type="com.riil.base.pojo.dict.DictCollFrequencyPojo">
		<result property="day" column="C_DAY" javaType="string" jdbcType="VARCHAR" />
		<result property="desc" column="C_DESC" javaType="string" jdbcType="VARCHAR" />
		<result property="frequency" column="C_FREQUENCY" javaType="_int" jdbcType="INTEGER" />
		<result property="frequencyUnit" column="C_FREQUENCY_UNIT" javaType="string" jdbcType="VARCHAR" />
		<result property="hour" column="C_HOUR" javaType="string" jdbcType="VARCHAR" />
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="metricType" column="C_METRIC_TYPE" javaType="string" jdbcType="VARCHAR" />
		<result property="minute" column="C_MINUTE" javaType="string" jdbcType="VARCHAR" />
		<result property="month" column="C_MONTH" javaType="string" jdbcType="VARCHAR" />
		<result property="name" column="C_NAME" javaType="string" jdbcType="VARCHAR" />
		<result property="nameEn" column="C_NAME_EN" javaType="string" jdbcType="VARCHAR" />
		<result property="sortId" column="C_SORT_ID" javaType="_int" jdbcType="SMALLINT" />
		<result property="type" column="C_TYPE" javaType="_byte" jdbcType="TINYINT" />
		<result property="week" column="C_WEEK" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	<!-- Select All POJO object -->
	<select id="select_all" resultMap="dictCollFrequencyResultMap">
		SELECT C_DAY, C_DESC, C_FREQUENCY, C_FREQUENCY_UNIT, C_HOUR, C_ID,
		C_METRIC_TYPE, C_MINUTE, C_MONTH, C_NAME, C_NAME_EN,
		C_SORT_ID,
		C_TYPE, C_WEEK
		FROM t_dict_coll_frequency
	</select>
</mapper>
