<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.CmdsProcessorDao">

	<resultMap id="cmdsProcessorResultMap" type="com.riil.base.binding.pojo.CollectCmdsProcessor">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="cmdsGroupId" column="C_CMDS_GROUP_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="className" column="C_CLASSNAME" javaType="string" jdbcType="VARCHAR" />
		<result property="method" column="C_METHOD" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	<select id="select_by_all"  resultMap="cmdsProcessorResultMap">
		SELECT  C_ID, C_CMDS_GROUP_ID,C_CLASSNAME,C_METHOD
		FROM
		t_moni_cmds_processor
    </select>
	
	<select id="select_by_cmdsId" parameterType="string" resultMap="cmdsProcessorResultMap">
		SELECT  C_ID, C_CMDS_GROUP_ID,C_CLASSNAME,C_METHOD
		FROM
		t_moni_cmds_processor
		WHERE C_CMDS_GROUP_ID = #{cmdsGroupId ,jdbcType=VARCHAR}
    </select>
	
</mapper>
