<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.ConnProtocolDao">

	<resultMap id="collectCmdsConnResultMap" type="com.riil.base.binding.pojo.CollectCmdsConnProtocol">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="cmdsGroupId" column="C_CMDS_GROUP_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="protocol" column="C_PROTOCOL" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	
	<select id="select_by_all"  resultMap="collectCmdsConnResultMap">
		SELECT C_ID, C_CMDS_GROUP_ID,C_PROTOCOL
		FROM
		t_moni_cmds_conn_protocol
    </select>
	
	<select id="select_by_collectCmdsId" parameterType="string" resultMap="collectCmdsConnResultMap">
		SELECT C_ID, C_CMDS_GROUP_ID,C_PROTOCOL
		FROM
		t_moni_cmds_conn_protocol
		WHERE C_CMDS_GROUP_ID = #{cmdsGroupId ,jdbcType=VARCHAR}
    </select>
	
</mapper>
