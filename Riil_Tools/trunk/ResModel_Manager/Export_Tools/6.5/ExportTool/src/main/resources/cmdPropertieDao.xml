<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.CmdPropertieDao">

	<resultMap id="cmdPropertieResultMap" type="com.riil.base.binding.pojo.CmdPropertie">
		<result property="cmdId" column="C_CMD_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="name" column="C_NAME" javaType="string" jdbcType="VARCHAR" />
		<result property="value" column="C_VALUE" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	<select id="select_by_all" resultMap="cmdPropertieResultMap">
		SELECT C_CMD_ID,C_NAME,C_VALUE
		FROM
		t_moni_cmd_properties
    </select>
	<select id="select_by_collectCmdId" parameterType="string" resultMap="cmdPropertieResultMap">
		SELECT C_CMD_ID,C_NAME,C_VALUE
		FROM
		t_moni_cmd_properties
		WHERE C_CMD_ID = #{cmdId ,jdbcType=VARCHAR}
    </select>
	
</mapper>
