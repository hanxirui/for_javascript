<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.resmodel.dao.impl.ResModelDao">

	<resultMap id="modelBaseResultMap" type="com.riil.base.resmodel.pojo.base.ModelBasePojo">
		<result property="connectInfoDesc" column="C_CONNECT_INFO_DESC" javaType="string" jdbcType="VARCHAR" />
		<result property="desc" column="C_DESC" javaType="string" jdbcType="VARCHAR" />
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="isMain" column="C_IS_MAIN" javaType="_byte" jdbcType="TINYINT" />
		<result property="isSnmp" column="C_IS_SNMP" javaType="_byte" jdbcType="TINYINT" />
		<result property="resTypeId" column="C_RES_TYPE_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="name" column="C_NAME" javaType="string" jdbcType="VARCHAR" />
		<result property="path" column="C_PATH" javaType="string" jdbcType="VARCHAR" />
		<result property="pluginId" column="C_PLUGIN_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="precondition" column="C_PRECONDITION" javaType="string" jdbcType="VARCHAR" />
		<result property="preconditionEn" column="C_PRECONDITION_EN" javaType="string" jdbcType="VARCHAR" />
		<result property="mainModelId" column="C_MAIN_MODEL_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="tag1" column="C_TAG1" javaType="string" jdbcType="VARCHAR" />
		<result property="tag2" column="C_TAG2" javaType="string" jdbcType="VARCHAR" />
		<result property="tag3" column="C_TAG3" javaType="string" jdbcType="VARCHAR" />
		<result property="tag4" column="C_TAG4" javaType="string" jdbcType="VARCHAR" />
		<result property="treeNodeId" column="C_TREE_NODE_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="vendorId" column="C_VENDOR_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="vendorName" column="C_VENDOR_NAME" javaType="string" jdbcType="VARCHAR" />
	</resultMap>

	<!-- Select All POJO object -->
	<select id="select_all" parameterType="com.riil.base.resmodel.pojo.base.ModelBasePojo" resultMap="modelBaseResultMap">
		SELECT C_CONNECT_INFO_DESC,   C_DESC,  
		C_ID, C_IS_MAIN, C_IS_SNMP, C_RES_TYPE_ID,
		C_NAME,  
		C_PLUGIN_ID, C_PRECONDITION,  C_MAIN_MODEL_ID,
		C_TAG1,
		C_TAG2, C_TAG3,
		C_TAG4, C_TREE_NODE_ID, C_VENDOR_ID, C_VENDOR_NAME
		FROM t_moni_model_base
		<trim prefix="ORDER BY">
			<if test="orderBy != null and orderBy != ''">
				${orderBy}
			           </if>
		</trim>
	</select>
</mapper>
