<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.base.resmodel.impl.db.dao.CollectCmdDao">

	<resultMap id="collectCmdResultMap" type="com.riil.base.binding.pojo.CollectCmdPojo">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="collectCmdsId" column="C_CMDS_GROUP_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="index" column="C_INDEX" javaType="string" jdbcType="VARCHAR" />
		<result property="protocol" column="C_PROTOCOL" javaType="string" jdbcType="VARCHAR" />
		<result property="cmd" column="C_CMD" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	
	<insert id="batchInsert"  parameterType="list" >
		INSERT INTO t_moni_cmd (  C_ID, C_CMDS_GROUP_ID,C_INDEX,C_PROTOCOL,C_CMD)
		VALUES
		<foreach collection="list" item="item" separator=",">
			( 
			#{item.id ,jdbcType=VARCHAR},#{item.collectCmdsId ,jdbcType=VARCHAR},#{item.index ,jdbcType=VARCHAR},#{item.protocol ,jdbcType=VARCHAR},#{item.cmd ,jdbcType=VARCHAR}
			)
		</foreach>
	</insert>
	
</mapper>
