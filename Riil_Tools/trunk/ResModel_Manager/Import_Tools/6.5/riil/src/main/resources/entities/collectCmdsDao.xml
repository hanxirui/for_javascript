<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.base.resmodel.impl.db.dao.CollectCmdsDao">

	<resultMap id="metricBindingResultMap" type="com.riil.base.binding.pojo.CollectCmdsPojo">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="metricBindingId" column="C_METRICBINDING_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="isDefault" column="C_IS_DEFAULT" javaType="byte" jdbcType="TINYINT" />
		<result property="isDynamic" column="C_IS_DYNAMIC" javaType="byte" jdbcType="TINYINT" />
		<result property="sortId" column="C_SORT_ID" javaType="string" jdbcType="INTEGER" />
	</resultMap>
	
	<insert id="batchInsert"  parameterType="list" >
		INSERT INTO t_moni_cmds_group (  C_ID, C_METRICBINDING_ID,C_IS_DEFAULT,C_IS_DYNAMIC,C_SORT_ID)
		VALUES
		<foreach collection="list" item="item" separator=",">
			( 
			#{item.id ,jdbcType=VARCHAR},#{item.metricBindingId ,jdbcType=VARCHAR},#{item.isDefault ,jdbcType=VARCHAR},#{item.isDynamic ,jdbcType=VARCHAR},#{item.sortId ,jdbcType=INTEGER}
			)
		</foreach>
	</insert>
	
</mapper>
