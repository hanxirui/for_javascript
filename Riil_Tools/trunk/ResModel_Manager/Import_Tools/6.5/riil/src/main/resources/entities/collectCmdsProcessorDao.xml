<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.base.resmodel.impl.db.dao.CollectCmdsProcessorDao">

	<resultMap id="metricBindingResultMap" type="com.riil.base.binding.pojo.CollectCmdsProcessor">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="cmdsGroupId" column="C_CMDS_GROUP_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="className" column="C_CLASSNAME" javaType="string" jdbcType="VARCHAR" />
		<result property="method" column="C_METHOD" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	
	<insert id="batchInsert"  parameterType="list" >
		INSERT INTO t_moni_cmds_processor (  C_ID, C_CMDS_GROUP_ID,C_CLASSNAME,C_METHOD)
		VALUES
		<foreach collection="list" item="item" separator=",">
			( 
			#{item.id ,jdbcType=VARCHAR},#{item.cmdsGroupId ,jdbcType=VARCHAR},#{item.className ,jdbcType=VARCHAR},#{item.method ,jdbcType=VARCHAR}
			)
		</foreach>
	</insert>
	
</mapper>
