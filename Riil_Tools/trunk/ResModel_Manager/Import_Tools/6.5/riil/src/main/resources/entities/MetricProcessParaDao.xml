<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.base.resmodel.impl.db.dao.MetricProcessParaDao">

	<resultMap id="metricBindingResultMap" type="com.riil.base.binding.pojo.MetricProcessPara">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="metricBindingId" column="C_CMDS_PROCESSOR_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="parameter" column="C_PARAMETER" javaType="string" jdbcType="VARCHAR" />
		<result property="sortId" column="C_SORT_ID" javaType="string" jdbcType="INTEGER" />
	</resultMap>
	
	<insert id="batchInsert"  parameterType="list" >
		INSERT INTO t_moni_metric_process_para (C_ID,C_METRICBINDING_ID,C_PARAMETER,C_SORT_ID)
		VALUES
		<foreach collection="list" item="item" separator=",">
			(
			#{item.id ,jdbcType=VARCHAR},#{item.metricBindingId ,jdbcType=VARCHAR},#{item.parameter ,jdbcType=VARCHAR},#{item.sortId ,jdbcType=INTEGER}
			)
		</foreach>	
	</insert>
	
</mapper>
