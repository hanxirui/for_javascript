	<sql id="Query_Where_Clause">
		<trim prefix="WHERE" prefixOverrides="AND |OR ">
			<if test="isMain != null and isMain!=0 ">
				AND C_IS_MAIN = #{isMain ,jdbcType=TINYINT}
            </if>

			<if test="isSnmp != null and isSnmp!=0 ">
				AND C_IS_SNMP = #{isSnmp ,jdbcType=TINYINT}
            </if>

			<if test="resTypeId != null and resTypeId != ''">
				AND C_RES_TYPE_ID = #{resTypeId ,jdbcType=VARCHAR}
            </if>

			<if test="treeNodeId != null and treeNodeId != ''">
				AND C_TREE_NODE_ID like <include refid="func_concat_1" /> #{treeNodeId ,jdbcType=VARCHAR}<include refid="func_concat_2" />  
            </if>

			<if test="name != null and name != ''">
				AND C_NAME = #{name ,jdbcType=VARCHAR}
            </if>

			<if test="vendorName != null and vendorName != ''">
				AND C_VENDOR_NAME = #{vendorName ,jdbcType=VARCHAR}
            </if>
            
            <if test="mainModelId != null and mainModelId != ''">
				AND C_MAIN_MODEL_ID = #{mainModelId ,jdbcType=VARCHAR}
            </if>
            
			<if test=" tag4 != null and tag4 == 'hasRes'">
				AND C_ID IN (SELECT DISTINCT c_model_id FROM t_res_instance_info)
            </if>
		</trim>
	</sql>

