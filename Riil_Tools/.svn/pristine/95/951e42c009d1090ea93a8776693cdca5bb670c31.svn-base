<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.base.resmodel.impl.db.dao.CmdSupportDao">

	<resultMap id="cmdSupportResultMap" type="com.riil.base.binding.pojo.CmdSupportPojo">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="cmdsGroupId" column="C_CMDS_GROUP_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="version" column="C_VERSION" javaType="string" jdbcType="VARCHAR" />
		<result property="rel" column="C_REL" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	
	<insert id="batchInsert"  parameterType="list" >
		INSERT INTO t_moni_cmds_support (  C_ID, C_CMDS_GROUP_ID,C_VERSION,C_REL)
		VALUES
		<foreach collection="list" item="item" separator=",">
			( 
			#{item.id ,jdbcType=VARCHAR},#{item.cmdsGroupId ,jdbcType=VARCHAR},#{item.version ,jdbcType=VARCHAR},#{item.rel ,jdbcType=VARCHAR}
			)
		</foreach>
	</insert>
	
</mapper>
