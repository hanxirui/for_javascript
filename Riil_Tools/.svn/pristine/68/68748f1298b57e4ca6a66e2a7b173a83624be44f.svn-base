				<input type="hidden" id="userName" value="${userName}">
				<input type="hidden" id="roleType" value="${roleType}">

				<ul>
					<li class="bg_gray con_b">基础信息</li>
					<li class="m_14">
						<table width="80%" cellspacing="0" cellpadding="0" class="t_td">
							<tr>
								<td width="100" height="26">模型ID：</td>
								<td><input name="ModelId" type="text" id=ModelId size="60" onblur="checkModelId()"style="width:503px"/>
