<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.base.resmodel.impl.db.dao.CollectCmdsConnProtocolDao">

	<resultMap id="collectCmdsConnResultMap" type="com.riil.base.binding.pojo.CollectCmdsConnProtocol">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="cmdsGroupId" column="C_CMDS_GROUP_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="protocol" column="C_PROTOCOL" javaType="string" jdbcType="VARCHAR" />
	</resultMap>
	
	<insert id="batchInsert"  parameterType="list" >
		INSERT INTO t_moni_cmds_conn_protocol (  C_ID, C_CMDS_GROUP_ID,C_PROTOCOL)
		VALUES
		<foreach collection="list" item="item" separator=",">
			( 
			#{item.id ,jdbcType=VARCHAR},#{item.cmdsGroupId ,jdbcType=VARCHAR},#{item.protocol ,jdbcType=VARCHAR}
			)
		</foreach>
	</insert>
	
</mapper>
