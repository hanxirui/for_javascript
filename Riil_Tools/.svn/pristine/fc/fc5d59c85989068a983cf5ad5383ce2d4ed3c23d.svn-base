<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.riil.base.resmodel.impl.db.dao.MetricBindingDao">

	<resultMap id="metricBindingResultMap" type="com.riil.base.binding.pojo.MetricBindingPojo">
		<result property="id" column="C_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="modelId" column="C_MODEL_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="metricId" column="C_METRIC_ID" javaType="string" jdbcType="VARCHAR" />
		<result property="method" column="C_METHOD" javaType="string" jdbcType="VARCHAR" />
		<result property="className" column="C_CLASSNAME" javaType="string" jdbcType="VARCHAR" />
		<result property="isDisplayName" column="C_IS_DISPLAYNAME" javaType="byte" jdbcType="TINYINT" />
		<result property="isInitValue" column="C_IS_INITVALUE" javaType="byte" jdbcType="TINYINT" />
		<result property="isInstance" column="C_IS_INSTANCE" javaType="byte" jdbcType="TINYINT" />
	</resultMap>
	
	<insert id="batchInsert"  parameterType="list" >
		INSERT INTO t_moni_metricbinding (  C_ID, C_MODEL_ID,C_METRIC_ID,C_METHOD,C_CLASSNAME,C_IS_DISPLAYNAME,C_IS_INITVALUE,C_IS_INSTANCE)
		VALUES
		<foreach collection="list" item="item" separator=",">
			( 
			#{item.id ,jdbcType=VARCHAR},#{item.modelId ,jdbcType=VARCHAR},#{item.metricId ,jdbcType=VARCHAR},#{item.method ,jdbcType=VARCHAR},#{item.className ,jdbcType=VARCHAR},
			#{item.isDisplayName ,jdbcType=VARCHAR},#{item.isInitValue ,jdbcType=VARCHAR},#{item.isInstance ,jdbcType=VARCHAR}
			)
		</foreach>
	</insert>
	
</mapper>
