		<property name="resourcePolicyService" ref="resourcePolicyService" />
		<property name="scriptPolicyService" ref="scriptPolicyService" />
		<property name="resUniteService" ref="resUniteService" />
	</bean>
	
	<bean id="dictService" class="com.riil.base.resmodel.impl.DictService">
